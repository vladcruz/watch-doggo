{
  "refresh_interval": 300,
  "timeout": 30,
  "services": [
    {
      "name": "github",
      "url": "https://www.githubstatus.com/api/v2/status.json",
      "adapter": "statuspage_io",
      "display_name": "GitHub",
      "category": "Development"
    },
    {
      "name": "twilio",
      "url": "https://status.twilio.com/api/v2/status.json",
