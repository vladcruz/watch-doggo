      "url": "https://status.cloud.google.com/incidents.json",
      "adapter": "api_adapter",
      "display_name": "Google Maps",
      "category": "Production",
      "status_path": "incidents",
      "status_mapping": {
        "open": "down",
        "closed": "operational"
